	<span class="n">string</span> <span class="n">header</span> <span class="o">=</span> <span class="s">"HTML Header"</span><span class="p">;</span>
	<span class="n">string</span> <span class="n">footer</span> <span class="o">=</span> <span class="s">"HTML Footer"</span><span class="p">;</span>

<span class="nl">public:</span>
	<span class="kt">void</span> <span class="n">show</span><span class="p">(</span><span class="kt">void</span><span class="p">)</span> <span class="p">{</span>
		<span class="n">cout</span> <span class="o">&lt;&lt;</span> <span class="k">this</span><span class="o">-&gt;</span><span class="n">header</span> <span class="o">&lt;&lt;</span> <span class="s">"</span><span class="se">\t</span><span class="s">"</span> 
			<span class="o">&lt;&lt;</span> <span class="k">this</span><span class="o">-&gt;</span><span class="n">footer</span> <span class="o">&lt;&lt;</span> <span class="n">endl</span><span class="p">;</span>
	<span class="p">}</span>
