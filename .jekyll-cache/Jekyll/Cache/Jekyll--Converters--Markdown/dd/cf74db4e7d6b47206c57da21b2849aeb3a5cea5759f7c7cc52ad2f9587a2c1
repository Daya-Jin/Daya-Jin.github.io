	<span class="n">string</span> <span class="n">info</span> <span class="o">=</span> <span class="s">"Detail Info"</span><span class="p">;</span>

<span class="nl">public:</span>
	<span class="kt">void</span> <span class="n">show</span><span class="p">(</span><span class="kt">void</span><span class="p">)</span> <span class="p">{</span>    <span class="c1">// å­ç±»ä¼šé‡å†™çˆ¶ç±»çš„åŒåå‡½æ•°</span>
		<span class="n">cout</span> <span class="o">&lt;&lt;</span> <span class="k">this</span><span class="o">-&gt;</span><span class="n">header</span> <span class="o">&lt;&lt;</span> <span class="s">"</span><span class="se">\t</span><span class="s">"</span>
